{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "1.1.0",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@electron-forge/cli": "6.2.1",
    "@electron-forge/maker-appx": "6.2.1",
    "@electron-forge/maker-deb": "6.2.1",
    "@electron-forge/maker-rpm": "6.2.1",
    "@electron-forge/maker-squirrel": "6.2.1",
    "@electron-forge/maker-wix": "6.2.1",
    "@electron-forge/maker-zip": "6.2.1",
    "@electron-forge/plugin-webpack": "6.2.1",
    "@types/cron": "2.0.1",
    "@types/node": "18.15.3",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "electron": "25.3.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "5.0.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "node-loader": "2.0.0",
    "prettier": "3.0.0",
    "sass": "1.64.0",
    "sass-loader": "13.3.2",
    "style-loader": "3.3.3",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "webpack": "5.88.2"
  },
  "dependencies": {
    "@ant-design/icons": "5.1.4",
    "antd": "5.7.1",
    "cron": "2.3.1",
    "effector": "22.8.6",
    "effector-react": "22.5.3",
    "electron-log": "5.0.0-beta.24",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.1.0",
    "i18next": "23.2.11",
    "i18next-fs-backend": "2.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "13.0.2",
    "react-router-dom": "6.14.2"
  }
}
