{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "2.0.4",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".vite/build/index.js",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --skipLibCheck --noEmit",
    "check:lint": "eslint --config=eslint.config.js",
    "check:test": "jest --config=jest.config.js"
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron-forge/cli": "7.7.0",
    "@electron-forge/maker-appx": "7.7.0",
    "@electron-forge/maker-deb": "7.7.0",
    "@electron-forge/maker-rpm": "7.7.0",
    "@electron-forge/maker-squirrel": "7.7.0",
    "@electron-forge/maker-wix": "7.7.0",
    "@electron-forge/maker-zip": "7.7.0",
    "@electron-forge/plugin-fuses": "7.7.0",
    "@electron-forge/plugin-vite": "7.7.0",
    "@electron/fuses": "1.8.0",
    "@jest/globals": "29.7.0",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/node": "20.17.19",
    "@types/opener": "1.4.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/semver": "7.5.8",
    "cross-env": "7.0.3",
    "electron": "34.2.0",
    "eslint": "9.21.0",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.5.2",
    "sass": "1.85.1",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.25.0",
    "vite": "6.2.0"
