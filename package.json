{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "2.0.4",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".vite/build/index.js",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --skipLibCheck --noEmit",
    "check:lint": "eslint --config=eslint.config.js",
    "check:test": "jest --config=jest.config.js"
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron-forge/cli": "7.6.0",
    "@electron-forge/maker-appx": "7.6.0",
    "@electron-forge/maker-deb": "7.6.0",
    "@electron-forge/maker-rpm": "7.6.0",
    "@electron-forge/maker-squirrel": "7.6.0",
    "@electron-forge/maker-wix": "7.6.0",
    "@electron-forge/maker-zip": "7.6.0",
    "@electron-forge/plugin-fuses": "7.6.0",
    "@electron-forge/plugin-vite": "7.6.0",
    "@electron/fuses": "1.8.0",
    "@jest/globals": "29.7.0",
    "@types/electron-squirrel-startup": "1.0.2",
    "@types/node": "20.17.6",
    "@types/opener": "1.4.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/semver": "7.5.8",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "electron": "33.2.0",
    "eslint": "9.15.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "globals": "15.12.0",
    "jest": "29.7.0",
    "prettier": "3.4.1",
    "sass": "1.81.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "vite": "6.1.0"
  },
  "dependencies": {
    "antd": "5.22.2",
    "cache-manager": "6.2.0",
    "cron": "3.2.1",
    "effector": "23.2.3",
    "effector-react": "23.2.1",
    "electron-log": "5.2.3",
    "electron-squirrel-startup": "1.0.1",
    "electron-store": "8.2.0",
    "i18next": "24.0.2",
    "i18next-fs-backend": "2.3.2",
    "mime": "4.0.4",
    "opener": "1.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.1.2",
    "react-router": "7.0.1",
    "semver": "7.6.3"
  }
