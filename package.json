    "effector": "22.5.0",
    "effector-react": "22.5.0",
    "electron-log": "5.0.0-beta.19",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.1.0",
    "i18next": "22.4.9",
    "i18next-fs-backend": "2.1.1",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "12.1.5",
    "react-router-dom": "6.8.1"
  }
