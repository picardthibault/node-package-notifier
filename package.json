    "electron-log": "5.1.7",
    "electron-squirrel-startup": "1.0.1",
    "electron-store": "8.2.0",
    "i18next": "23.12.2",
    "i18next-fs-backend": "2.3.1",
    "mime": "4.0.4",
    "opener": "1.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.0.0",
    "react-router-dom": "6.25.1"
