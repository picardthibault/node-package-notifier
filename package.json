{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "1.2.0",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --skipLibCheck --noEmit",
    "check:lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@electron-forge/cli": "7.2.0",
    "@electron-forge/maker-appx": "7.2.0",
    "@electron-forge/maker-deb": "7.2.0",
    "@electron-forge/maker-rpm": "7.2.0",
    "@electron-forge/maker-squirrel": "7.2.0",
    "@electron-forge/maker-wix": "7.2.0",
    "@electron-forge/maker-zip": "7.2.0",
    "@electron-forge/plugin-webpack": "7.2.0",
    "@types/node": "18.18.14",
    "@types/opener": "1.4.3",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "7.0.3",
    "css-loader": "6.10.0",
    "electron": "28.2.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "node-loader": "2.0.0",
    "prettier": "3.2.4",
    "sass": "1.70.0",
    "sass-loader": "14.1.0",
    "style-loader": "3.3.4",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.90.0"
  },
  "dependencies": {
    "antd": "5.13.3",
    "cache-manager": "5.4.0",
    "cron": "3.1.6",
    "effector": "23.1.0",
    "effector-react": "23.1.0",
    "electron-log": "5.1.0",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.1.0",
    "i18next": "23.8.1",
    "i18next-fs-backend": "2.3.1",
    "opener": "1.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "14.0.1",
    "react-router-dom": "6.21.3"
  }
}
