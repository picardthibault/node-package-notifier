{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "1.1.0",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-react": "7.18.6",
    "@electron-forge/cli": "6.0.5",
    "@electron-forge/maker-appx": "6.0.5",
    "@electron-forge/maker-deb": "6.0.5",
    "@electron-forge/maker-rpm": "6.0.5",
    "@electron-forge/maker-squirrel": "6.0.5",
    "@electron-forge/maker-wix": "6.0.5",
    "@electron-forge/maker-zip": "6.0.5",
    "@electron-forge/plugin-webpack": "6.0.5",
    "@types/cron": "2.0.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "electron": "23.1.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "node-loader": "2.0.0",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "typescript": "~4.5.4",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@ant-design/icons": "5.1.4",
