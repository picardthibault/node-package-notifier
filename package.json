{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "2.0.3",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --skipLibCheck --noEmit",
    "check:lint": "eslint --config=eslint.config.js"
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/preset-react": "7.25.9",
    "@electron-forge/cli": "7.5.0",
    "@electron-forge/maker-appx": "7.5.0",
    "@electron-forge/maker-deb": "7.5.0",
    "@electron-forge/maker-rpm": "7.5.0",
    "@electron-forge/maker-squirrel": "7.5.0",
    "@electron-forge/maker-wix": "7.5.0",
    "@electron-forge/maker-zip": "7.5.0",
    "@electron-forge/plugin-webpack": "7.5.0",
    "@types/node": "20.17.6",
    "@types/opener": "1.4.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "9.2.1",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "electron": "33.2.0",
    "eslint": "9.14.0",
    "eslint-plugin-react": "7.37.2",
    "eslint-plugin-react-hooks": "5.0.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "globals": "15.12.0",
    "mini-css-extract-plugin": "2.9.2",
    "node-loader": "2.0.0",
    "prettier": "3.3.3",
    "sass": "1.80.6",
    "sass-loader": "16.0.3",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "typescript-eslint": "8.14.0",
    "webpack": "5.96.1"
  },
  "dependencies": {
    "antd": "5.22.0",
    "cache-manager": "6.1.3",
    "cron": "3.2.0",
    "effector": "23.2.3",
    "effector-react": "23.2.1",
    "electron-log": "5.2.2",
    "electron-squirrel-startup": "1.0.1",
    "electron-store": "8.2.0",
    "i18next": "23.16.5",
    "i18next-fs-backend": "2.3.2",
    "mime": "4.0.4",
    "opener": "1.5.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "15.1.1",
    "react-router-dom": "6.28.0"
  }
