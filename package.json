{
  "name": "node-package-notifier",
  "productName": "NPN",
  "version": "2.1.0",
  "description": "A simple Electron Js app that notify new npm package release.",
  "main": ".webpack/main",
  "scripts": {
    "start": "cross-env ENVIRONMENT=DEV electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --parser typescript --write *.ts src/**/*.ts src/**/*.tsx",
    "check:format": "prettier --parser typescript --list-different *.ts src/**/*.ts src/**/*.tsx",
    "check:types": "tsc --skipLibCheck --noEmit",
    "check:lint": "eslint ."
  },
  "keywords": [],
  "author": "Thibault Picard",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/preset-react": "7.24.7",
    "@electron-forge/cli": "7.4.0",
    "@electron-forge/maker-appx": "7.4.0",
    "@electron-forge/maker-deb": "7.4.0",
    "@electron-forge/maker-rpm": "7.4.0",
    "@electron-forge/maker-squirrel": "7.4.0",
    "@electron-forge/maker-wix": "7.4.0",
    "@electron-forge/maker-zip": "7.4.0",
    "@electron-forge/plugin-webpack": "7.4.0",
    "@types/node": "20.14.9",
    "@types/opener": "1.4.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "12.0.2",
    "cross-env": "7.0.3",
    "css-loader": "7.1.2",
    "electron": "31.1.0",
    "eslint": "9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-prettier": "5.1.3",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "globals": "15.6.0",
    "mini-css-extract-plugin": "2.9.0",
    "node-loader": "2.0.0",
    "prettier": "3.3.2",
    "sass": "1.77.6",
    "sass-loader": "14.2.1",
    "style-loader": "4.0.0",
    "ts-loader": "9.5.1",
